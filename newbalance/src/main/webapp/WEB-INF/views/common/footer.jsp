    <script>
      function fnLayerPopOpenForContentsPolicy() {
        event.preventDefault();
        $("#contentsPolicyPop").css(
          "top",
          Math.max(0, ($(window).height() - 500) / 2 + $(window).scrollTop()) +
            "px"
        );
        $("#contentsPolicyPop").css(
          "left",
          Math.max(0, ($(window).width() - 780) / 2 + $(window).scrollLeft()) +
            "px"
        );

        $("#contentsPolicyPop").show();
        $("#contentsPolicyPopDimmed").show();
      }
      function fnLayerPopCloseForContentsPolicy() {
        event.preventDefault();
        $("#contentsPolicyPop").hide();
        $("#contentsPolicyPopDimmed").hide();
      }
    </script>